{
    "name": "hypnotract/laravel-view-identifiers",
    "version": "5.6.0",
    "description": "A quick way to set global classes and global id to main HTML DOM element in Laravel 5.",
    "keywords": [
        "laravel",
        "view",
        "global",
        "identifiers",
        "class",
        "id"
    ],
    "homepage": "https://github.com/hypnotract/laravel-view-identifiers",
    "authors": [
        {
            "name": "Denis Dostali",
            "email": "hypnotract@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": ">=5.6"
    },
    "autoload": {
        "psr-4": {
            "Hypnotract\\ViewIdentifier\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hypnotract\\ViewIdentifier\\ServiceProvider"
            ],
            "aliases": {
                "ViewIdentifier": "Hypnotract\\ViewIdentifier\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
